    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@types/node-fetch": "^2.6.1",
    "convert-audio": "^1.0.1",
    "dotenv": "^16.0.3",
    "essentia.js": "^0.1.3",
    "fs": "^0.0.1-security",
    "node-downloader-helper": "^2.1.1",
    "node-fetch": "^2.6.7",
    "node-id3": "^0.2.3",
    "node-wav": "^0.0.2",
    "python-shell": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
