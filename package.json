    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.3",
    "filereader": "^0.10.3",
    "fluent-ffmpeg": "^2.1.2",
    "jest": "^27.2.5",
    "jsdom": "^19.0.0",
    "mp3-to-wav": "^0.1.11",
    "node-lame": "^1.3.2",
    "parcel": "2.4.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tone": "^14.7.77",
